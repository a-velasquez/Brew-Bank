<!DOCTYPE html>
<html>
  <head>
    <title>BrewBank</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  
  <body>

    <ul class="nav nav-pills nav-justified">
      <li class="nav-item">
        <a class="nav-link">
          <%= navigation_links %>
        </a>
      </li>
    </ul>

    
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
